{
  "name": "oly-dialog",
  "version": "1.0.0",
  "description": "An angular wrapper for the HTML5 <dialog> element",
  "main": "./dist/oly-dialog.min.js",
  "scripts": {
    "install": "browserify --debug src/index.js -t [babelify --sourceMapRelative .] --outfile dist/oly-dialog.js && uglifyjs dist/oly-dialog.js -o dist/oly-dialog.min.js",
