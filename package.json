  "license": "MIT",
  "dependencies": {
    "angular": "^1.3.14",
    "jquery": "^2.1.3"
  },
  "devDependencies": {
    "babelify": "^5.0.4",
    "browserify": "^9.0.3",
    "uglify-js": "^2.4.16"
  },
  "bugs": {
    "url": "https://github.com/olympicsoftware/oly-dialog/issues"
  },
  "homepage": "https://github.com/olympicsoftware/oly-dialog"
